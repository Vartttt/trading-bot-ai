
## .env.example
```ini
# --- Exchange ---
EXCHANGE=mexc3           # mexc3 | binance
DEFAULT_TYPE=swap        # swap=futures
API_KEY=your_api_key
API_SECRET=your_api_secret

# --- Modes ---
DRY_RUN=True
CHECK_INTERVAL=30
PHASE_REFRESH_MIN=30
MIN_STRENGTH=72
DEFAULT_LEVERAGE=10
MIN_NOTIONAL=5

# --- Dynamic Position Sizing ---
BASE_RISK=0.015
TARGET_VOL=0.015
SIZE_STRENGTH_MIN=70
SIZE_STRENGTH_MAX=95
SIZE_CAP_RISK=0.03

# --- Liquidity/Spread ---
MIN_BOOK_BIDS=3
MIN_BOOK_ASKS=3
MAX_SPREAD_PCT=0.08
MIN_24H_USDT_VOL=200000

# --- Correlation / Exposure ---
CORR_LOOKBACK_HOURS=24
MAX_ACTIVE_CORR=0.85
MAX_GLOBAL_POS=6
MAX_FAMILY_POS=1

# --- TP/SL / trailing ---
TP1_RATIO=0.5
TP2_RATIO=0.3
RUNNER_RATIO=0.2
TRAIL_ATR_K=1.2
TRAIL_MIN_SECONDS=120
MOVE_SL_TO_BE_AFTER_TP1=True

# --- Daily Risk Cap & Cooldown ---
MAX_DAILY_LOSS_PCT=0.02
DAILY_COOLDOWN_MIN=180

# --- Alpha Guards ---
TRADING_SESSION_UTC=7-22
NEWS_WINDOWS_UTC=
MAX_FUNDING_ABS=0.003

# --- Execution / Router ---
MAKER_FIRST=True
MAKER_TIMEOUT_SEC=6
MAKER_REBID_SEC=3
MAX_RETRY=3
RETRY_SLEEP_SEC=1.2
FEE_RATE_BPS=7

# --- Dynamic Symbols ---
DYNAMIC_SYMBOLS=True
DYNSYM_TOPN=12
DYNSYM_CANDIDATES=60
DYNSYM_LIQ_USDT=2000000
DYNSYM_MAX_SPREAD=0.001
DYNSYM_MAX_CORR=0.82
DYNSYM_REFRESH_MIN=60
DYNSYM_CACHE_PATH=state/dynamic_symbols.json

# --- Telegram ---
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
